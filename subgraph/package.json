{
  "name": "@scaffold-eth/subgraph",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.thegraph.com/deploy/ --ipfs https://api.thegraph.com/ipfs/ GITHUB_USERNAME/your-contract",
    "create-local": "graph create --node http://localhost:8020/ ghostffcode/eth-kit",
    "remove-local": "graph remove --node http://localhost:8020/ ghostffcode/eth-kit",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 ghostffcode/eth-kit",
    "reset-local": "yarn build && yarn codegen && yarn remove-local && yarn create-local && yarn deploy-local"
  },
  "dependencies": {
    "@graphprotocol/graph-cli": "^0.29.1",
    "@graphprotocol/graph-ts": "^0.26.0"
  },
  "devDependencies": {
    "mustache": "^3.1.0"
  }
}
